# - Config file for the peli package
# It defines the following variables
#  PELI_INCLUDE_DIRS - include directories for Peli
#  PELI_LIBRARIES    - libraries to link against

# Compute paths
get_filename_component(PELI_CMAKE_FILES_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(PELI_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET peli AND NOT peli_BINARY_DIR)
  include("${PELI_CMAKE_FILES_DIR}/peli-targets.cmake")
endif()

# These are IMPORTED targets created by peli-targets.cmake
set(PELI_LIBRARIES peli)
